	ipb.global.registerReputation( 'rep_post_402513', { domLikeStripId: 'like_post_402513', app: 'forums', type: 'pid', typeid: '402513' }, parseInt('5') );
</script>
				
								
				
				
				
			
				<ul id="postControlsNormal_402513" class="post_controls clear clearfix">
					<li class="top hide"><a href="#ipboard_body" class="top" title="Наверх">Наверх</a></li>
					
					
					
					
					
					
				</ul>
					
			</div>
		</div>
